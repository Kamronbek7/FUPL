def FaylMavjudEmasligiXato(b):
	return "X 1==\nFl-X:","'"+b+"' Fl- "

def FaylMavjudligiXato(b):
	return "X 1==\nFl+X: + '"+b+"' Fl"

def ModulMavjudEmasligiXato(b):
	return "X 1==\nM-X: M '"+b+"' -"

def NomXato(b):
	return "X 1==\nNX: N '"+b+"'-"

def QiymatXato(b):
	return "X: 1 ==\nQX: '"+b+"' ~No"

def SintaksisXato(b):
 return "X: 1==\nSX:\n\t"+b
 
def ModulMavjudEmasligiXato(a):
	return "X: 1==\nM-X: M \'"+"\'-"

# BOSHQA
def output(a):
	a=str(a)
	f=open(a,"r")
	n=f.read()
	f.close()
	try:
		print(n,"\n")
		exec(n)
		return ''
	except SyntaxError:
		return "SX:\n"+n
	except NameError:
		return "NX: N '"+a+"' -"
def natija(a):
	return output(str(a))

def file(a):# YOZISH
  	f=str(a)
  	s=0
  	open(f,"w"). close()
  	while True:
  		r=open(f,"a")
  		s+=1
  		d=input(str(s)+"__|")
  		if "save"in d or "saqla"in d :
  			return output(f)
  		else:
  			"save" not in d and "saqla"not in d
  			r.write(d+"\n");r.close()


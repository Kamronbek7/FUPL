# SOʻZLAR
def mirr(a):
	a=str(a)
	return a[::-1]
def aks(a):
	a=str(a)
	return a[::-1]

def uzun(a):
	a=str(a)
	return len(a)

def da(a,b):
	a=str(a);b=str(b)
	if b in a:
		return True
	if b not in a:
		return False

def tur(a):
	return type(a)

def kirit(*a):
	return input(a)

def pas():
	None

# CASElar
def if_t_t(a,b):
	if a==b:
		return True
	else:
		return False

# VAQT, SOAT
def hozir():
	return datetime.now()
def now():
	return hozir()

def pas():
	pass

# STANDART OPERATORLAR
def yozuv(*a):
	print(*a)

def kirit(*a):
	return input(*a)

def hisob(a):
	return eval(str(a))
def tkshr(*a):
  return exec(str(a))

# IDE lar
def ide():
  Salom='yozuv("Salom, dunyo!")'
  vaqt='yozuv(hozir())'
  kalkulyator='a=kirit(\'misol/sum: \')\nyozuv(hisob(a))'

  yozuv('ASSALOMU ALAYKUM!\nBu yerda siz ajoyib tayyor IDElarni topishingiz mumkin.\nIDE nomlari quyidagilar:\n\nHELLO!\n You can find ready IDEs in this function\nName of IDEs in down: ')
  yozuv(' "Salom", "vaqt", "kalkulyator", "", \nShulardan birini tanlang./You select:')
  while True:
    q=input('\nIDE nomi/Name of IDE: ')
    if 'Salom' in q:
      yozuv('Code:\n'+Salom+'\nNatija/Output:\n')
      exec(Salom)
    if 'vaqt'in q:
      yozuv('Code:\n'+vaqt+'\nNatija/Output:')
      exec(vaqt)
    if 'kalkulyator' in q:
      yozuv('Code:\n'+kalkulyator+'\nNatija/Output:')
      exec(kalkulyator)
    if 'chiqish'in q or 'quit'in q:
      return '';break
    else:
      continue

